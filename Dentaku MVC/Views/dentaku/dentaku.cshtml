@model Dentaku_MVC.Models.dentakuModel

@{
    ViewBag.Title = "電卓もどき";
}
<h2>@ViewBag.Title</h2>

@using (@Html.BeginForm())
{
    <div class="form-item">
        @Html.DisplayNameFor(model => model.siki)
        @Html.TextBoxFor(model => model.siki)
    </div>
    <div class="form-item">
        @Html.DisplayNameFor(model => model.result)
        @Html.DisplayFor(model => model.result)
    </div>
    <div class="button-panel">
        <input type="submit" class="button" value="計算" />
    </div>
    <br />
    @ViewBag.ErrorMessage
    <label style="color:#ff0000">@Html.ValidationSummary()</label>
}
